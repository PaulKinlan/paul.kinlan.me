{
  "public": true,
  "framework": "hugo",
  "trailingSlash": true,
  "redirects": [
    {
      "source": "/amp/(.+)/",
      "destination": "/$1/"
    }
  ],
  "headers": [
    {
      "source": "/(.*)/webfinger",
      "headers": [
        {
          "key": "content-type",
          "value": "application/jrd+json; charset=utf-8"
        }
      ]
    },
    {
      "source": "/(.*).ajson",
      "headers": [
        {
          "key": "content-type",
          "value": "application/activity+json"
        }
      ]
    }
  ]
}
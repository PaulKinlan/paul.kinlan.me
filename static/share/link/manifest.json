{
  "name": "Blog: Link Share",
  "short_name": "Blog: Link Share",
  "start_url": "/share/link/",
  "theme_color": "#000000",
  "background_color": "#000000",
  "icons": [ {
      "sizes": "192x192",
      "src": "/images/me.png",
      "type": "image/png"
  }],
  "share_target": {
    "url_template": "/share/link/?title={title}&text={text}&url={url}",
    "action": "submit",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "display": "standalone",
  "scope": "/share/link/"
}

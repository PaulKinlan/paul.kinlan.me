{
  "name": "paul.kinlan.me",
  "alias": [
    "paul.kinlan.me"
  ],
  "static": {
    "headers": [
      {
        "source" : "**/*.@(mjs)",
        "headers" : [{
          "key" : "Content-type",
          "value" : "text/javascript"
        }]
      },
      {
        "source" : "**/*.@(jpg|jpeg|gif|png)",
        "headers" : [{
          "key" : "Cache-Control",
          "value" : "max-age=7200"
        }]
      }
    ]
  }
}
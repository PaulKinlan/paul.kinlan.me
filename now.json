{
  "name": "paul.kinlan.me",
  "alias": [
    "paul.kinlan.me"
  ],
  "static": {
    "redirects": [
      { "source": "/amp/:id", "destination": "/:id", "type": 301 }
    ],  
    "headers": [
      {
        "source" : "**/*.@(mjs)",
        "headers" : [{
          "key" : "Content-type",
          "value" : "text/javascript"
        }]
      },
      {
        "source" : "**/*.@(jpg|jpeg|gif|png)",
        "headers" : [{
          "key" : "Cache-Control",
          "value" : "max-age=7200"
        }]
      },
      {
        "source" : "**/*.@(js|json)",
        "headers" : [{
          "key" : "Cache-Control",
          "value" : "max-age=7200"
        }]
      }
    ]
  }
}
